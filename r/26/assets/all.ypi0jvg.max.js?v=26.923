      var ret
      if (options.allowLoop) {
        ret = inverse ?
          ( $current_slide.prev()[0] || $slides.last()[0]  ) : 
          ( $current_slide.next()[0] || $slides.first()[0] )
      } else {
        ret = inverse ? 
          ( $current_slide.prev()[0] || $slides.first()[0] ) : 
          ( $current_slide.next()[0] || $slides.last()[0]  )
      }
    
      return $(ret);
    };


